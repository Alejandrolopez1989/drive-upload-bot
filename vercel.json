{
  "version": 2,
  "builds": [
    { "src": "bot.py", "use": "@vercel/python" }
  ],
  "routes": [
    { "src": "/webhook", "dest": "bot.py", "headers": { "Cache-Control": "no-store" } },
    { "src": "/set-webhook", "dest": "bot.py", "headers": { "Cache-Control": "no-store" } },
    { "src": "/get-webhook-info", "dest": "bot.py", "headers": { "Cache-Control": "no-store" } }
  ]
}
